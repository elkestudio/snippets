{
  
  "CSS starter": {
    "prefix": "startit",
    "body": [
      "/* ##Device = Desktops ##Screen = 1281px to higher resolution desktops */",
      "",
      "@media screen and (min-width: 1281px) {",
      "",
      "}",
      "",
      "/* ##Device = Laptops, Desktops ##Screen = B/w  to 1280px */",
      "",
      "@media screen and (max-width: 1280px) {",
      "",
      "}",
      "",
      "/* ##Device = Tablets, Ipads (portrait) ##Screen = B/w  to 1024px */",
      "",
      "@media screen and (max-width: 1024px) {",
      "",
      "}",
      "",
      "/* ##Device = Tablets, Ipads (landscape) ##Screen = B/w  to 1024px */",
      "",
      "@media screen and (max-width: 1024px) and (orientation: landscape) {",
      "",
      "}",
      "",
      "/* ##Device = Low Resolution Tablets, Mobiles (Landscape) ##Screen = B/w to 768px */",
      "",
      "@media screen and (max-width: 768px) {",
      "",
      "}",
      "",
      "/* ##Device = Most of the Smartphones Mobiles (Portrait) ##Screen = B/w  to 479px */",
      "",
      "@media screen and (max-width: 480px) {",
      "",
      "}",
      "/* ##Device = Small Smartphones Mobiles (Portrait) ##Screen = B/w  to 320px */",
      "",
      "@media screen and (max-width: 320px) {",
      "",
      "}"
    ],
    "description": "CSS starter with media queries"
  },

  "CSS center": {
    "prefix": "centerme",
    "body": [
      "position:absolute;",
      "top:50%;",
      "left:50%;",
      "transform: translate(-50%, -50%);"
    ],
    "description": "CSS Center "
  },

  "CSS full width": {
    "prefix": "full",
    "body": [
      "width: 100%;",
      "height: 100vh;"
    ]
  },

  "section hero navigation": {
    "prefix": "heronav",
    "body": [
      " @import 'colors.scss';",
      "",
      "#outlet{",
      "z-index: 1000;",
      "}",
      "",
      ".dim {",
      "position: fixed;",
      "background-color: rgba(245, 245, 245, 0.7);",
      "top: 0;",
      "left: 0;",
      "width: 100%;",
      "height: 100%;",
      "}",
      "",
      ".menu-button {",
      "position: fixed;",
      "top: 5%;",
      "right: 3%;",
      "width: 40px;",
      "z-index: 101;",
      "cursor: pointer;",
      "",
      ".line1 {",
      "width: 80 % ;",
      "height: 2 px;",
      "background: $primary;",
      "}",
      "",
      ".line2 {",
      "width: 65 % ;",
      "height: 2 px;",
      "background: $primary;",
      "}",
      "",
      ".line3 {",
      "width: 100 % ;",
      "height: 2 px;",
      "background: $primary;",
      "}",
      "}",
      "",
      ".menu-wrapper {",
      "width: 100%;",
      "height: 1px;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: white;",
      "z-index: 100;",
      "",
      ".menu {",
      "text-align: center;",
      "transform: translateY(-100%);",
      "",
      ".menu-item {",
      "position: relative;",
      "opacity: 0;",
      "width: 100% ;",
      "cursor: pointer;",
      "",
      "&:hover.line {",
      "opacity: 0.7;",
      "transform: scaleX(5);",
      "}",
      "",
      "h1 {",
      "color: $primary;",
      "font-size: 30px;",
      "}",
      "",
      ".line {",
      "position: absolute;",
      "bottom: 42%;",
      "width: 100px;",
      "height: 1px;",
      "opacity: 0;",
      "background: $primary-light;",
      "transition: all .6s;",
      "}",
      "}",
      "}",
      "}",
      "",
      ".nav-back1 {",
      "width: 100%;",
      "height: 0;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: white;",
      "z-index: 300;",
      "}",
      "",
      ".nav-back2 {",
      "width: 100%;",
      "height: 0;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: #B9B9B9;",
      "z-index: 200;",
      "}",
      "",
      ".nav-back3 {",
      "width: 100%;",
      "height: 0;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: #6D6D6D;",
      "z-index: 100;",
      "}",
      "",
      ".menu-list {",
      "text-align: center;",
      "width: 100%;",
      "height: 64px;",
      "position: fixed;",
      "top: 0;",
      "right: 3%;",
      "z-index: 100;",
      "",
      ".menu-list-item {",
      "position: relative;",
      "opacity: 0;",
      "cursor: pointer;",
      "",
      "&:hover .line {",
      "opacity: 0.7;",
      "transform: scaleX(1.4);",
      "}",
      "",
      "h1 {",
      "color: $primary-dark;",
      "font-size: 20px;",
      "}",
      "",
      ".line {",
      " position: absolute;",
      "bottom: 41%;",
      "width: 70px;",
      "height: 2px;",
      "opacity: 0;",
      "background: $white;",
      "transition: all .6s;",
      "}",
      "}",
      "}"
    ]
  }


}