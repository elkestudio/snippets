{

  "SCSS breakpoints": {
    "prefix": "breakpoints",
    "body": [
      "// Screen size variables //",
      "",
      "\\$screen-ixs-min: 200px; // Idiotically tiny phones",
      "\\$screen-xxs-min: 320px; // Really tiny phones",
      "\\$screen-xs-min: 425px; // Tiny phones",
      "\\$screen-sm-min: 576px; // Small tablets and large smartphones (landscape view)",
      "\\$screen-md-min: 768px; // Small tablets (portrait view)",
      "\\$screen-lg-min: 992px; // Tablets and small desktops",
      "\\$screen-xl-min: 1200px; // Large tablets and desktops",
      "\\$screen-xxl-min: 2000px; // Extra large tablets and desktops",
      "",
      "// $screen-iPad-1 : 768px;",
      "// $screen-iPad-2 : 1024px;",
      "",
      "// $screen-iPadPro-1 : 1024px;",
      "// $screen-iPadPro-2 : 1366px;",
      "",
      "// Mixins //",
      "",
      "//PORTRAIT",
      "@mixin  iPad {",
      " @media (min-width: 768px) and (max-width: 1024px) and (orientation : portrait) and (-webkit-min-device-pixel-ratio: 2) {",
      "@content;",
      "}",
      "}",
      "@mixin  iPadPro {",
      "@media (min-width: 1024px) and (max-width: 1366px) and (orientation : portrait) and (-webkit-min-device-pixel-ratio: 2) {",
      "@content;",
      "}",
      "}",
      "//LANDSCAPE",
      "@mixin iPadLandscape {",
      "@media (min-width: 768px) and (max-width: 1024px) and (orientation: landscape) and (-webkit-min-device-pixel-ratio: 2) {",
      "@content;",
      "}",
      "}",
      "",
      "@mixin iPadProLandscape {",
      " @media (min-width: 1024px) and (max-width: 1366px) and (orientation: landscape) and (-webkit-min-device-pixel-ratio: 2) {",
      "@content;",
      "}",
      "}",
      "",
      "// Idiotically tiny devices 200px",
      "@mixin ixs {",
      "@media (min-width: #{\\$screen-ixs-min}) {",
      "@content;",
      "}",
      "}",
      "// Really tiny devices 320px",
      "@mixin xxs {",
      "@media (min-width: #{\\$screen-xxs-min}) {",
      "@content;",
      "}",
      "}",
      "// Tiny devices 425px",
      "@mixin xs {",
      "@media (min-width: #{\\$screen-xs-min}) {",
      "@content;",
      "}",
      "}",
      "",
      "// Small devices 576px",
      "@mixin sm {",
      "@media (min-width: #{\\$screen-sm-min}) {",
      "@content;",
      "}",
      "}",
      "",
      "// Medium devices 768px",
      "@mixin md {",
      "@media (min-width: #{\\$screen-md-min}) {",
      "@content;",
      "}",
      "}",
      "",
      "// Large devices 992px",
      "@mixin lg {",
      "@media (min-width: #{\\$screen-lg-min}) {",
      "@content;",
      "}",
      "}",
      "",
      "// Extra large devices 1200px",
      "@mixin xl {",
      "@media (min-width: #{\\$screen-xl-min}) {",
      "@content;",
      "}",
      "}",
      "",
      "// Extra Extra large devices 2000px",
      "@mixin xxl {",
      " @media (min-width: #{\\$screen-xxl-min}) {",
      "@content;",
      "}",
      "}"

    ],
    "description": "SCSS breakpoints"
  },

  "center": {
    "prefix": "center",
    "body": [
      "position:absolute;",
      "top:50%;",
      "left:50%;",
      "transform: translate(-50%, -50%);"
    ]
  },
  "position absolute": {
    "prefix": "absolute",
    "body": [
      "position: absolute;",
      "top:0;",
      "right:0"
    ]
  },
  "position relative": {
    "prefix": "relative",
    "body": [
      "position: relative;",
      "top:0;",
      "right:0"
    ]
  },
  "full width & height": {
    "prefix": "full",
    "body": [
      "width: 100%;",
      "height: 100vh;"
    ]
  },
  "width": {
    "prefix": "width",
    "body": [
      "height: $1;"
    ]
  },
  "height": {
    "prefix": "height",
    "body": [
      "height: $1;"
    ]
  },
  "margin": {
    "prefix": "margin",
    "body": [
      "margin: $1px $2px $3px $4px; //top right bottom left"
    ]
  },
  "padding": {
    "prefix": "padding",
    "body": [
      "padding: $1px $2px $3px $4px //top right bottom left;"
    ]
  },
  "image wrapper": {
    "prefix": "imgwrap",
    "body": [
      ".img-wrapper {",
      "display: flex;",
      "width: $1;",
      "height: $1;",
      " }",
      "",
      ".img {",
      "width: 100%;",
      "object-fit: cover;",
      "}"
    ]
  },
	"colors": {
	  "prefix": "colors",
	  "body": [
	    "\\$primary: $1;",
	    "\\$primary-dark: darken(\\$primary, 20%);",
	    "\\$primary-light: lighten(\\$primary, 30%);",
	    "\\$secondary: $2;",
	    "\\$secondary-dark: darken(\\$secondary, 20%);",
	    "\\$secondary-light: lighten(\\$secondary, 30%);",
	    "\\$accent: $3;",
	    "\\$accent-dark: darken(\\$accent, 20%);",
	    "\\$accent-light: lighten(\\$accent, 30%);",
	    "\\$warn: $4;",
	    "\\$warn-dark: darken(\\$warn, 20%);",
	    "\\$warn-light: lighten(\\$warn, 30%);",
	  ]
	},


  "section hero navigation": {
    "prefix": "heronav",
    "body": [
      " @import 'colors.scss';",
      "",
      "#outlet{",
      "z-index: 1000;",
      "}",
      "",
      ".dim {",
      "position: fixed;",
      "background-color: rgba(245, 245, 245, 0.7);",
      "top: 0;",
      "left: 0;",
      "width: 100%;",
      "height: 100%;",
      "}",
      "",
      ".menu-button {",
      "position: fixed;",
      "top: 5%;",
      "right: 3%;",
      "width: 40px;",
      "z-index: 101;",
      "cursor: pointer;",
      "",
      ".line1 {",
      "width: 80%;",
      "height: 2px;",
      "background: $primary;",
      "}",
      "",
      ".line2 {",
      "width: 65%;",
      "height: 2px;",
      "background: $primary;",
      "}",
      "",
      ".line3 {",
      "width: 100%;",
      "height: 2px;",
      "background: $primary;",
      "}",
      "}",
      "",
      ".menu-wrapper {",
      "width: 100%;",
      "height: 1px;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: white;",
      "z-index: 100;",
      "",
      ".menu {",
      "text-align: center;",
      "transform: translateY(-100%);",
      "",
      ".menu-item {",
      "position: relative;",
      "opacity: 0;",
      "width: 100% ;",
      "cursor: pointer;",
      "",
      "&:hover.line {",
      "opacity: 0.7;",
      "transform: scaleX(5);",
      "}",
      "",
      "h1 {",
      "color: $primary;",
      "font-size: 30px;",
      "}",
      "",
      ".line {",
      "position: absolute;",
      "bottom: 42%;",
      "width: 100px;",
      "height: 1px;",
      "opacity: 0;",
      "background: $primary-light;",
      "transition: all .6s;",
      "}",
      "}",
      "}",
      "}",
      "",
      ".nav-back1 {",
      "width: 100%;",
      "height: 0;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: white;",
      "z-index: 300;",
      "}",
      "",
      ".nav-back2 {",
      "width: 100%;",
      "height: 0;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: #B9B9B9;",
      "z-index: 200;",
      "}",
      "",
      ".nav-back3 {",
      "width: 100%;",
      "height: 0;",
      "position: fixed;",
      "top: 0;",
      "right: 0;",
      "background: #6D6D6D;",
      "z-index: 100;",
      "}",
      "",
      ".menu-list {",
      "text-align: center;",
      "width: 100%;",
      "height: 64px;",
      "position: fixed;",
      "top: 0;",
      "right: 3%;",
      "z-index: 100;",
      "",
      ".menu-list-item {",
      "position: relative;",
      "opacity: 0;",
      "cursor: pointer;",
      "",
      "&:hover .line {",
      "opacity: 0.7;",
      "transform: scaleX(1.4);",
      "}",
      "",
      "h1 {",
      "color: $primary-dark;",
      "font-size: 20px;",
      "}",
      "",
      ".line {",
      " position: absolute;",
      "bottom: 41%;",
      "width: 70px;",
      "height: 2px;",
      "opacity: 0;",
      "background: $white;",
      "transition: all .6s;",
      "}",
      "}",
      "}"
    ]
  }


}